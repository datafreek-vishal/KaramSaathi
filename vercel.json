{
  "functions": {
    "src/app/api/auth/[...nextauth]/route.ts": {
      "maxDuration": 10
    }
  },
  "env": {
    "SKIP_ENV_VALIDATION": "true",
    "NODE_ENV": "production"
  },
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "regions": ["iad1"],
  "outputDirectory": ".next",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./"
}
